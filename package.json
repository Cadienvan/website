{
  "name": "mateonunez-frontend-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.js",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
    "pretty": "prettier --write .",
    "quality": "npm run lint && npm run format"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint"
    ],
    "*.{js,jsx,json,html,css,scss,svg}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "next": "^12.1.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@elgorditosalsero/react-gtm-hook": "^2.4.0",
    "@mdx-js/loader": "^2.1.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.44",
    "autoprefixer": "^10.4.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "classnames": "^2.3.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mdx": "^1.17.0",
    "eslint-plugin-next": "0.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gray-matter": "^4.0.3",
    "gsap": "^3.10.4",
    "husky": "^7.0.4",
    "moment": "^2.29.3",
    "next-mdx-remote": "^4.0.3",
    "next-seo": "^5.4.0",
    "next-sitemap": "^2.5.20",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.12",
    "prettier": "^2.4.1",
    "query-string": "^7.1.1",
    "react-lottie-player": "^1.4.2",
    "reading-time": "^1.5.0",
    "rehype-prism-plus": "^1.3.2",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remove-markdown": "^0.3.0",
    "sass": "^1.51.0",
    "sharp": "^0.30.4",
    "swr": "^1.3.0",
    "tailwindcss": "^3.0.24",
    "terser": "^5.13.0",
    "three": "^0.139.0",
    "url-join": "^5.0.0"
  },
  "author": "mateonunez"
}
